pipeline 
{
agent any
stages
{
stage ('scm checkout')
{
steps
{
git 'https://github.com/shaileejain09/maven-project'
}
}
stage ('test my project')
{
steps
{
withMaven(jdk: 'JAVA', maven: 'maven tool') {
    sh 'mvn install'
}
stage{
steps{
sshagent (['tomcatnew']) {
    sh 'scp -o StrictHostKeyChecking=no */target/*.war ec2-user@3.113.19.253:/var/lib/tomcat/webapps/'
  }
}
}
    
}
}
}}
